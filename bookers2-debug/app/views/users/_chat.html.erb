<% unless @user.id == current_user.id %>
  <% if (current_user.is_followed_by? @user) && (@user.is_followed_by? current_user) %>
    <% if @isRoom == true %>
      <% link_to "chatを始める" room_path(@room_id) %>
    <% else %>
      <%= form_with @room do |f| %>
        <%= fields_for @entry do |e| %>
          <%= e.hidde_field :user_id, value: @user.id %>
        <% end %>
        <%= f.submit "チャットを始める" %>
      <% end %>
    <% end %>
  <% end %>
<% end %>